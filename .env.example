# Stripe Configuration
# Get your keys from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_secret_key_here
PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Stripe Webhook Secret (for local testing use Stripe CLI)
# Get from: stripe listen --forward-to localhost:5173/api/webhooks/stripe
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Stripe Product/Price IDs (create these in Stripe Dashboard)
STRIPE_PRICE_ID_MONTHLY=price_monthly_id_here
STRIPE_PRICE_ID_YEARLY=price_yearly_id_here

# Supabase Configuration
# Get from https://app.supabase.com/project/_/settings/api
PUBLIC_SUPABASE_URL=https://your-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Application URL
PUBLIC_APP_URL=http://localhost:5173
